# Server Configuration
NODE_ENV=development
PORT=3000

# MongoDB Configuration
MONGO_URI=mongodb://localhost:27017/product_catalog

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_should_be_long_and_secure
JWT_EXPIRES_IN=90d

# Rate Limiting
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info

# CORS
CORS_ORIGIN=*

# Note: For production, you should set these environment variables
# in your deployment environment, not in a .env file